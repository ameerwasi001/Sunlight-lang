open unit: s, r
unit ? r@SltList -> [s]

open bind: s, f
bind ? s@SltList -> (fold: (\a, b -> a .. b), (head: ls), tail: ls where ls <- fmap: f, s end)

join: s <- s bind id

class mapM: f, r, xs
[] = xs -> unit: [], r
true -> (f: head: xs) bind \y -> (mapM: f, r, tail: xs) bind \ys -> unit: ([y] .. ys), r

mapM_: f, r, xs <- (mapM: f, r, xs) bind \unit: (), r