f: x <- x*2
fcx: f <- \f, x -> change: x, 1, f <| \f, x -> change: x, 2, f <| f
mdLs <- \f -> change: a, 2, f <| fcx <| f

mod Shapes
    type Shape <- Circle{r} | Square{e}
    class getV: s
    s@Shapes::Circle -> s.r*2
    s@Shapes::Square -> s.e*2
    a <- 10
end

circ <- Shapes::Circle{r :: 15}

out <- mlp2 seq (some: \x+Shapes::getV: circ) seq mdLs[2][1][2] where mlp2 <- some: \x*2 end
